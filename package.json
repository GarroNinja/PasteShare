{
  "name": "pasteshare",
  "version": "1.0.0",
  "description": "A pastebin-like application for sharing code snippets and text",
  "main": "index.js",
  "scripts": {
    "start": "node server/dist/server.js",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "dev": "bash ./start.sh",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "build": "cd client && npm run build && cd ../server && npm run build",
    "postinstall": "node vercel-pg-fix.js",
    "vercel-build": "node vercel-pg-fix.js"
  },
  "keywords": [
    "pastebin",
    "code-sharing",
    "text-sharing"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1"
  },
  "peerDependencies": {
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=16.x"
  }
}
